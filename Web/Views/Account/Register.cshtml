@model RegisterViewModel

@{
    ViewBag.Title = "User Registration";
}
<head>
    <!-- Vendor CSS Files -->
    <link href="~/profile/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/profile/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/profile/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/profile/assets/css/about.css" rel="stylesheet">



</head>
<section id="about" class="about-mf sect-pt4 route" style="background-color: #9BA4B4;">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <div class="box-shadow-full">
                    <div class="row">

                        <div class="row">
                            <div>


                                <div class="row">
                                    <div class="col-md-6">
                                        <form method="post">
                                            <h1 style="text-align:center;">Create your Account</h1>
                                            <img src="~/assets/img/icons8-add-user-group-woman-man-64.png" style="float:right" />
                                            <br />
                                            <hr />
                                            <br />
                                            <div asp-validation-summary="All" class="text-danger"></div>
                                            <div class="form-group">
                                                <label asp-for="Name"></label>
                                                <input asp-for="Name" class="form-control" />
                                                <span asp-validation-for="Name" class="text-danger"></span>
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="Email"></label>
                                                <input asp-for="Email" class="form-control" />
                                                <span asp-validation-for="Email" class="text-danger"></span>
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="organization"></label>
                                                <input asp-for="organization" class="form-control" />
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="Password"></label>
                                                <input asp-for="Password" class="form-control" />
                                                <span asp-validation-for="Password" class="text-danger"></span>
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="ConfirmPassword"></label>
                                                <input asp-for="ConfirmPassword" class="form-control" />
                                                <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                                            </div><br /><br />
                                            <button type="submit" class="btn btn-primary">Register</button>
                                        </form>

                                        <div style="margin-top:15px">
                                            <p>
                                                <a asp-action="Login" asp-controller="Account">Have an Account? Login Here!</a>
                                            </p>
                                        </div>
                                    </div>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">

                            </div>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>

</section>
<!-- End About Section -->
<!-- ======= Clients Section ======= -->
<section id="clients" class="clients" style="background-color: #fff;">
    <div class="container" data-aos="fade-up">
    </div>
</section><!-- End Clients Section -->