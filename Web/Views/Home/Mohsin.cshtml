@model HomeViewModel

<head>
    <!-- Vendor CSS Files -->
    <link href="~/profile/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/profile/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/profile/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/profile/assets/css/about.css" rel="stylesheet">



</head>

<body>

    <!-- ======= About Section ======= -->
    <section id="about" class="about-mf sect-pt4 route" style="background-color: #9BA4B4;">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="box-shadow-full">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="row">
                                    <div class="col-sm-6 col-md-5">
                                        <div class="about-img">
                                            <img src="../assets/img/team/Mohsen.jpg" class="img-fluid rounded b-shadow-a" alt="">
                                        </div>
                                    </div>
                                    <div class="col-sm-6 col-md-7">
                                        <div class="about-info">
                                            <br><br>
                                            <p><span class="title-s">Name: </span> <span>Dr.Mohsin Hasan Hussein</span></p>
                                            <p><span class="title-s">The field:</span> <span>Social recommendation system</span></p>
                                            <p><span class="title-s">Email: </span> <span>mohsin.alqassab@gmail.com</span></p>
                                            <p><span class="title-s">Phone: </span> <span>...</span></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="skill-mf">
                                    <p class="title-s">Skill</p>
                                    <span>JAVA</span> <span class="pull-right">85%</span>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" style="width: 85%;" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                    <span>C++</span> <span class="pull-right">75%</span>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" style="width: 75%" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                    <span>C#</span> <span class="pull-right">50%</span>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" style="width: 50%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                    <span>HTML</span> <span class="pull-right">90%</span>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" style="width: 90%" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="about-me pt-4 pt-md-0">
                                    <div class="title-box-2">
                                        <h5 class="title-left">
                                            About me
                                        </h5>
                                    </div>
                                    <p class="lead">
                                        <B>Education :</B>  B.Sc. (Computer Science), University of Mosul, Mosul, IRAQ (2003).
                                        M.Sc. (Computer Science), Babylon University, Babylon, IRAQ (2012).
                                        Ph.D. (Computer Science), Babylon University, Babylon, IRAQ (2017).

                                    </p>
                                    <p class="lead">
                                        <B>POSITION HELD</B>
                                        -	Coordinator of Computer Science Department Kerbela (2017 – 2019).
                                        -	Vice Dean of the College of Computer Science & Information Technology (2020 – Now).

                                    </p>
                                    <p class="lead">
                                        <B>COURSES TAUGHT:</B>
                                        •	Data Mining
                                        •	Structured Programming.
                                        •	Object Oriented Programming
                                        •	Language Translator
                                        •	Computational Theory.
                                        •	Operating Systems
                                        •	Information Retrieval
                                        •	Data Structure
                                    </p>

                                    <p class="lead">
                                        <B>Research interests</B>
                                        •	Data Mining
                                        •	Recommandation System
                                        •	Information Filtering and Retrieval
                                        •	Social Network Analysis


                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section><!-- End About Section -->
    <!-- ======= Services Section ======= -->
    <section id="services" class="services section-bg" style="background-color: #9BA4B4; margin-top: -90px;">
        <div class="container">

            <div class="section-title" data-aos="fade-up">
                <h2>Publications</h2>
                <p>Here is all the research I've done.</p>
            </div>
            @foreach (var research in Model.researches)
            {
                if (research.owner2 == "Dr.Mohsin Hasan Hussein")
                {


                    <div data-aos="fade-up">

                        <div class="icon-box">
                            <div class="icon">
                                @research.year
                            </div>
                            <h4 class="title"><a asp-controller="Home" asp-action="Preview" asp-route-id="@research.Id">@research.ResearchName</a></h4>
                            <p class="description">
                                @research.Discription
                            </p>
                            <section id="blog" class="blog">
                                <div data-aos="fade-up" style="margin-left:-30px; margin-top: -23px;  color: #14274E; ">

                                    <div>

                                        <div>

                                            <article class="entry entry-single" style="box-shadow: none;">

                                                <div class="entry-meta">
                                                    <ul>
                                                        <li class="d-flex align-items-center"><i class="bi bi-person"></i> <a asp-action="Mohsin" asp-controller="Home">Dr.Mohsin Hasan Hussein</a></li>
                                                        <li class="d-flex align-items-center"><i class="bi bi-clock"></i> <a href=""><time datetime="2020-01-01">@research.year</time></a></li>
                                                        <li class="d-flex align-items-center"><i class="bi bi-chat-dots"></i> <a href="">@research.reads Reads</a></li>
                                                    </ul>
                                                </div>
                                            </article>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </div>
                    <br />

                }
            }
        </div>

    </section>
    <!-- ======= Clients Section ======= -->
    <section id="clients" class="clients" style="background-color: #fff;">
        <div class="container" data-aos="fade-up">
        </div>
    </section><!-- End Clients Section -->
</body>